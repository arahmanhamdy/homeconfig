[ui]
username = Jo De Boeck <jdeboeck@aserver.com>
editor = /usr/bin/vim -c "r ~/.hgtemplate"
merge = meld

[extensions]
fetch = 
churn =
color = 
extdiff = 
hgext.graphlog = 
purge = 
bookmarks = 
transplant = 
pager =
hgext.hgk=
rebase =

[hooks]
commit = python:flake8.run.hg_hook

[extdiff]
cmd.meld = 

[hgk]
meld=meld

[pager]
pager = less -R
attend = annotate, cat, df, diff, glog, help, in-diff, incoming, log, out-diff, outgoing, qdiff, show, tip, grep

[bookmarks]
track.current = True

[alias]
duw = push -r .
ditch = revert --no-backup
blame = annotate -u
