*background: black
*foreground: white
*cursorColor: red
*cursorBlink: true
*scrollBar: false
*boarderColor: red
*loginShell: true
*tintingType: true
*tinting: #ffffff
*tintColor: #FFFFFF
! black
*color0  : #2E3436
*color8  : #555753
! ! red
*color1  : #CC0000
*color9  : #EF2929
! ! green
*color2  : #4E9A06
*color10 : #8AE234
! ! yellow
*color3  : #C4A000
*color11 : #FCE94F
! ! blue
*color4  : #3465A4
*color12 : #729FCF
! ! magenta
*color5  : #75507B
*color13 : #AD7FA8
! ! cyan
*color6  : #06989A
*color14 : #34E2E2
! ! white
*color7  : #D3D7CF
*color15 : #EEEEEC
*matcher.button: 3
*colorUL: #86A2BB
urxvt.url-launcher: /usr/bin/firefox
urxvt.url-select.launcher: /usr/bin/firefox
urxvt.url-select.underline: false
urxvt.font: xft:Monospace:size=8
urxvt.perl-ext-common: default,clipboard,keyboard-select,confirm-paste,url-select,selection-autotransform,font-size
urxvt.matcher.pattern.1: \\w{8}-\\w{4}-\\w{4}-\\w{4}-\\w{12}
urxvt.matcher.pattern.2: (https?|ftps?)://[^\\s]+
urxvt.keysym.Mod4-t: perl:keyboard-select:activate
urxvt.keysym.Mod4-u: perl:url-select:select_next
urxvt.keysym.Mod4-v: perl:clipboard:paste
urxvt.keysym.Mod4-c: perl:clipboard:copy
urxvt.keysym.Mod4-minus: perl:font-size:decrease
urxvt.keysym.Mod4-equal: perl:font-size:increase
urxvt.shading:    20
urxvt.transparent:true

urxvt.selection-autotransform.0: s/^([^:[:space:]]+)(\\.py|\\.c|\\.js):(\\d+):?.*$/vi +$3 \\Q$1$2\\E\\x0d/
urxvt.cutchars:'"'&()*,;<=>?@[]^{|}
urxvt.termName: rxvt
urxvt.urgentOnBell: True
urxvt.saveLines: 10000
xterm.font: xft:Monospace:size=10
