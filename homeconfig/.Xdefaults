urxvt*termName: rxvt
urxvt*urgentOnBell: True
urxvt.saveLines: 10000
*background: black
*foreground: white
*cursorColor: red
*cursorBlink: true
*scrollBar: false
*boarderColor: red
*loginShell: true
*tintingType: true
*tinting: #ffffff
*tintColor: #FFFFFF
*shading: 10
! black
*color0  : #2E3436
*color8  : #555753
! ! red
*color1  : #CC0000
*color9  : #EF2929
! ! green
*color2  : #4E9A06
*color10 : #8AE234
! ! yellow
*color3  : #C4A000
*color11 : #FCE94F
! ! blue
*color4  : #3465A4
*color12 : #729FCF
! ! magenta
*color5  : #75507B
*color13 : #AD7FA8
! ! cyan
*color6  : #06989A
*color14 : #34E2E2
! ! white
*color7  : #D3D7CF
*color15 : #EEEEEC

*urlLauncher: firefox
*matcher.button: 3
*colorUL: #86A2BB
urxvt.font: xft:Monospace:size=8
URxvt.perl-ext-common: default,matcher,clipboard,keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.Mod4-v: perl:clipboard:paste
urxvt.cutchars:'"'&()*,;<=>?@[]^{|}
Xterm.font: xft:Monospace:size=10
